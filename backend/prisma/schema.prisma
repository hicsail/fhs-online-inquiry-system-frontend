  height              Decimal?  @db.Decimal
  weight              Int?
  bmi                 Decimal?  @db.Decimal
  sbp                 Decimal?  @db.Decimal
  dbp                 Decimal?  @db.Decimal
  vent_rt             Int?
  smoking             Int?
  cpd                 Int?
  bg                  Int?
  tc                  Int?
  triglycerides       Int?
  calc_ldl            Int?
  hdl                 Int?
