model dcdt_dates {
  framid    Int
  dcdt_date DateTime @db.Date
  num_dcdt  Int?
  sex       Int?

  @@id([framid, dcdt_date])
}

model derived_dcdt_dates {
  framid            Int
  derived_dcdt_date DateTime @db.Date
  num_derived_dcdt  Int?
  sex               Int?

  @@id([framid, derived_dcdt_date])
}

model dvoice_dates {
  framid      Int
  dvoice_date DateTime @db.Date
  sex         Int?
  

  @@id([framid, dvoice_date])
}

model mri_dates {
  framid   Int
  mri_date DateTime @db.Date
  sex      Int?
  

  @@id([framid, mri_date])
}

model participants {
  framid     Int          @id @default(autoincrement())
  cohort     Int?
  id         Int
  gender     Int
  race       String?      @db.VarChar
  edu_core2  Int?
  edu_core8  Int?
  mri_count  Int
  dvoice_count Int
  brain_data brain_data[] 
  core_tests core_tests[]
}

model survival {
  idtype        Int
  framid        Int
  date_of_death DateTime? @db.Date
  age_at_death  Int?

  @@id([idtype, framid])
}
